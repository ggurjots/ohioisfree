       <img src="https://images4.alphacoders.com/889/thumbbig-889823.webp"

       <bockquote> " I looked "</bockquote>
       <button onclick="changeQuote()">Change the quote</button>

       <script>
           var index = 0;
           function changeQuote() {
               var quotes = [
                   '"I really like web devolopment! - Gurjot',
                   '"im sleepy as frick - Gurjot',
                   '"naaaah bruh',
                   '"STYLLLLL',
               ]

               var blockquote = document.querySelector("blockquote");
               blockquote.innerHTML = quotes[index]

               index++;
               if (index>= 4){
                   index = 0;
               }
           }

       </script>
   </body>
